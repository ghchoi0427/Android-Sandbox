<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firebase210724">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Firebase210724"
        android:usesCleartextTraffic="true">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".BoardActivity" />
        <activity android:name=".UploadBoardActivity" />
        <activity android:name=".UploadScheduleActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ScheduleActivity" />
        <activity android:name=".WebActivity" />
        <activity android:name=".SocketClientActivity" />
    </application>

</manifest>